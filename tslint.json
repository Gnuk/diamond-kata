{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single", "avoid-escape", "avoid-template"],
        "indent": [true, "spaces", 2],
	"interface-name": false,
	"ordered-imports": true,
	"object-literal-sort-keys": false,
	"no-consecutive-blank-lines": true
    },
    "rulesDirectory": []
}
